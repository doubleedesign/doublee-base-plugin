{
	"name": "doubleedesign/doublee-base-plugin",
	"type": "wordpress-plugin",
	"version": "4.1.0",
	"homepage": "https://github.com/doubleedesign/doublee-base-plugin",
	"authors": [
		{
			"name": "Leesa Ward",
			"email": "leesa@doubleedesign.com.au"
		}
	],
	"require": {
		"php": ">=8.3"
	},
	"autoload": {
		"psr-4": {
			"Doubleedesign\\BasePlugin\\": [
				"src/"
			],
			"Doubleedesign\\BasePlugin\\Tests\\": [
				"tests/"
			]
		}
	},
	"require-dev": {
		"pestphp/pest": "^4.3",
		"brain/monkey": "^2.6",
		"sirbrillig/spies": "^1.10"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
